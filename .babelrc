{
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": "6",
          "browsers": [
            "last 2 versions"
          ]
        },
        "loose": false,
        "modules": false,
        "useBuiltIns": true
      }
    ],
    "es2015",
    "stage-0",
    "react"
  ],
  "env": {
    "dev": {
      "plugins": [
        "react-hot-loader/babel",
        "transform-decorators-legacy",
        "transform-object-rest-spread",
        "transform-class-properties",
        [
          "transform-runtime",
          {
            "polyfill": false
          }
        ]
      ]
    },
    "prod": {
      "plugins": [
        "transform-decorators-legacy",
        "transform-object-rest-spread",
        "transform-class-properties",
        [
          "transform-runtime",
          {
            "polyfill": false
          }
        ]
      ]
    }
  }
}
