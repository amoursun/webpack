  "description": "Webpack configuration to learn",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dest/dll-dev/* dest/dll-prod/*",
    "dll": "npm run clean && node config/webpack-config/webpack.config.dll.js",
    "build": "npm run clean && npm run dll && npm run prod",
    "prod": "rimraf dest/prod/* && cross-env NODE_ENV=prod webpack --config config/build-config/index.js",
    "dev": "node ./config/build-config/dev.server.js",
    "start": "npm run dev",
    "mock": "node ./mock/mock-start.js",
    "mock2": "node ./mock/mock2/mock-start.js",
